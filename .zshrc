#          --------------------------------
#          ZSHRC - Load modular configarion
#          --------------------------------

for f in ~/.config/zshrc/*; do
    if [ ! -d $f ]; then
        c=`echo $f | sed -e "s=.config/zshrc=.config/zshrc/custom="`
        [[ -f $c ]] && source $c || source $f
    fi
done

. "$HOME/.local/share/krita/ai_diffusion/server/uv/env"
